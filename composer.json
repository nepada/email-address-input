{
    "name": "nepada/email-address-input",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Email address form input for Nette forms.",
    "keywords": ["email address", "input", "form", "nette"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/email-address-input/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0 <8.3",
        "nepada/email-address": "^2.3@dev || ^3.0@dev",
        "nette/utils": "^3.2@dev || ^4.0@dev",
        "nette/forms": "^3.1@dev"
    },
    "require-dev": {
        "nette/tester": "2.4.3",
        "phpstan/phpstan": "1.10.22",
        "phpstan/phpstan-strict-rules": "1.5.1",
        "phpstan/phpstan-nette": "1.2.9",
        "nepada/phpstan-nette-tester": "1.0.0",
        "spaze/phpstan-disallowed-calls": "2.16.0",
        "shipmonk/phpstan-rules": "2.6.2",
        "php-parallel-lint/php-parallel-lint": "1.3.2",
        "nepada/coding-standard": "7.10.0",
        "nette/di": "^3.0.6@dev",
        "nette/schema": "^1.0.3@dev",
        "nette/bootstrap": ">=3.1@dev",
        "nette/component-model": ">=3.0.2",
        "nette/http": ">=3.1@dev"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\EmailAddressInput\\": "src/EmailAddressInput/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.8-dev"
        }
    }
}
