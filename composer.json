{
    "name": "nepada/email-address-input",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Email address form input for Nette forms.",
    "keywords": ["email address", "input", "form", "nette"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/email-address-input/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "nepada/email-address": "^2.3@dev || ^3.0@dev",
        "nette/utils": "^3.0@dev",
        "nette/forms": "^3.0@dev"
    },
    "require-dev": {
        "nette/tester": "^2.3.1",
        "phpstan/phpstan": "^0.12.26",
        "phpstan/phpstan-strict-rules": "^0.12.2",
        "pepakriz/phpstan-exception-rules": "^0.11.1",
        "phpstan/phpstan-nette": "^0.12.6",
        "nepada/phpstan-nette-tester": "^0.3.0",
        "spaze/phpstan-disallowed-calls": "^1.0",
        "php-parallel-lint/php-parallel-lint": "^1.1",
        "nepada/coding-standard": "^7.2",
        "nette/bootstrap": "^3.0@dev",
        "nette/di": "^3.0.1@dev",
        "nette/schema": "^1.0@dev",
        "nette/http": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\EmailAddressInput\\": "src/EmailAddressInput/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.6-dev"
        }
    }
}
